<Application x:Class="Poseidon.BackOffice.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
             xmlns:local="clr-namespace:Poseidon.BackOffice">
    <Application.Resources>
         <ResourceDictionary>
             <ResourceDictionary.MergedDictionaries>
                 
                 <ResourceDictionary Source="pack://application:,,,/Poseidon.BackOffice.Theme;component/Themes/Generic.xaml"/>
                 
                 <ResourceDictionary>
                    <local:AppBootstrapper x:Key="bootstrapper" />
                 </ResourceDictionary>
                 
             </ResourceDictionary.MergedDictionaries>
         </ResourceDictionary>
    </Application.Resources>
</Application>
