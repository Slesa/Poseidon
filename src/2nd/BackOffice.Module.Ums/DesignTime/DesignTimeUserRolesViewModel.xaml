    xmlns:ums="clr-namespace:Poseidon.Domain.Ums.Model;assembly=Poseidon.Domain.Ums.Model"
    mc:Ignorable="d">
    
    <ums:UserRole x:Key="Captain" Name="Caption"/>
    <ums:UserRole x:Key="Commander" Name="Commander"/>
    <ums:UserRole x:Key="Lieutenant" Name="Lieutenant"/>
    <ums:UserRole x:Key="LieutenantCmd" Name="Lieutenant Commander"/>
    
    <ViewModels:UserRolesViewModel x:Key="DesignTimeUserRolesViewModel" d:IsDataSource="True">
        <ViewModels:UserRolesViewModel.UserRoles>
            <ums:UserRole Name="Capatin" />
            <ums:UserRole Name="Lieutenant" />
            <ums:UserRole Name="Lieutenant Commander" />
        </ViewModels:UserRolesViewModel.UserRoles>
    </ViewModels:UserRolesViewModel>
    
</ResourceDictionary>