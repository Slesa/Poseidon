<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
    xmlns:ViewModels="clr-namespace:Poseidon.BackOffice.Module.Pms.ViewModels" 
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:pms="clr-namespace:Poseidon.Domain.Pms.Model;assembly=Poseidon.Domain.Pms.Model"
    mc:Ignorable="d">
    
    <ViewModels:DiscountsViewModel x:Key="DesignTimeDiscuntsViewModel" d:IsDataSource="True">
        <ViewModels:DiscountsViewModel.Discounts>
            <pms:Discount Name="10 Euro"  Rate="10" IsAbsolute="True" UseForSale="True" />
            <pms:Discount Name="Halbe"  Rate="0.5" UseForOrders="True" />
            <pms:Discount Name="Senior"  Rate="0.75" UseForOrders="True" />
        </ViewModels:DiscountsViewModel.Discounts>
     </ViewModels:DiscountsViewModel>
    
</ResourceDictionary>