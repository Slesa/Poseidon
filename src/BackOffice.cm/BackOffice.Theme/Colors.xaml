        <GradientStop Color="#FFC0D9FB" Offset="1"/>
        <GradientStop Color="#FFAFD1F8" Offset="0.53"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="AppModuleItemBackground" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="#FFFFBD69" Offset="0"/>
