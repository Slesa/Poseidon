<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:ics="clr-namespace:Poseidon.Domain.Ics.Model;assembly=Poseidon.Domain.Ics.Model"
    xmlns:ViewModels="clr-namespace:Poseidon.BackOffice.Module.Ics.ViewModels"
    mc:Ignorable="d">

    <ViewModels:StocksViewModel x:Key="DesignTimeStocksViewModel" d:IsDataSource="True">
        <ViewModels:StocksViewModel.Stocks>
            <ics:Stock Name="Hauptlager" IsMainStock="True" />
            <ics:Stock Name="Nebenlager" />
            <ics:Stock Name="Kasse" />
        </ViewModels:StocksViewModel.Stocks>
    </ViewModels:StocksViewModel>


</ResourceDictionary>
